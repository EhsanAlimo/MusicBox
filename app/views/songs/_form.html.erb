<%= form_for @song do |f|%>
    <%= f.label :Name%>
    <%= f.text_field :name, :required=>true%>
    <%= f.label :Genre%>
    <%= f.text_field :genre%>
    <%= f.label :Artist%>
    <%= f.text_field :artist%>
    <%= f.label :track%>
    <%= f.text_field :track%>
    <%= f.label :playlist_id%>
    <%= f.select :playlist_ids, Playlist.pluck(:name, :id), :include_blank => true%>
    <%= f.submit%>
<%end%>